<div class="container">
  <div class="row top-50 bottom-30">
    <div>
      <h2><%= current_user.user_info.first_name %>'s Skills</h2>
    </div>
    <div class="row top-20">
      <div class="skill-user-index">
        <%= render 'index' %>
      </div>
      <%= link_to 'Back', root_path, class: 'btn btn-primary' %>
      <%= link_to 'Add Skill', new_skill_path, remote: true, class: 'btn btn-primary' %>
    </div>
  </div>
</div>
<div id="skill-user-modal" class="modal fade">
    <div class="modal-dialog container">
      <div id="inner-skill-user-modal" class="modal-content row"></div>
    </div>
</div>
